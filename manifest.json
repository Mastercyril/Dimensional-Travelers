{
  "manifest_version": 3,
  "name": "Viverse PlayCanvas Extension",
  "version": "3.57.1",
  "description": "viverse-world-production-sso",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "webRequest",
    "system.cpu",
    "system.memory",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "*://*.playcanvas.com/*",
    "*://playcanvas.com/*"
  ],
  "action": {
    "default_popup": "src/index.html"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.content.tsx-6aa5c935-7d26b290.js"
      ],
      "matches": [
        "https://playcanvas.com/editor/scene/*"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "css": [
        "assets/content-24891668.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/*",
        "src/web_accessible_resources.js",
        "src/content.tsx",
        "src/sso/viverse-sdk.umd.staging.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://playcanvas.com/*"
      ],
      "resources": [
        "assets/viverse-sdk.umd.production-c20b54e2.js",
        "assets/viverse-sdk.umd.staging-c20b54e2.js",
        "assets/content.js"
      ],
      "use_dynamic_url": true
    }
  ]
}